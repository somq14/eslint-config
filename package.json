{
  "name": "@somq14/eslint-config",
  "version": "1.0.0",
  "main": "index.js",
  "packageManager": "yarn@3.2.2",
  "scripts": {
    "format": "prettier --ignore-path .gitignore --ignore-unknown --write \"**/*\"",
    "lint": "eslint --ignore-path .gitignore --max-warnings 0 \"**/*.{js,jsx,cjs,mjs,ts,tsx}\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "~5.33.0",
    "@typescript-eslint/parser": "~5.33.0",
    "eslint-config-prettier": "~8.5.0",
    "eslint-import-resolver-typescript": "~3.4.0",
    "eslint-plugin-import": "~2.26.0",
    "eslint-plugin-jsx-a11y": "~6.6.0",
    "eslint-plugin-only-warn": "~1.0.3",
    "eslint-plugin-react": "~7.30.0",
    "eslint-plugin-react-hooks": "~4.6.0",
    "eslint-plugin-sort-export-all": "~1.2.1"
  },
  "peerDependencies": {
    "eslint": "^7.2.0 || ^8.0.0",
    "typescript": ">=4.0.0 <4.7.5"
  },
  "devDependencies": {
    "eslint": "8.21.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "typescript": "4.7.4"
  }
}
